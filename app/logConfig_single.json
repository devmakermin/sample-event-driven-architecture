{
  "appenders": {
    "single_develop": {
      "type": "file",
      "filename": "SampleServer",
      "maxLogSize": 10485760,
      "backups": 336,
      "pattern": "yyyy-MM-dd-hh.log",
      "alwaysIncludePattern": true
    },
    "single_develop_console": { "type": "console" }
  },
  "categories": {
    "default": { "appenders": [ "single_develop", "single_develop_console" ], "level": "debug" }
  }
}